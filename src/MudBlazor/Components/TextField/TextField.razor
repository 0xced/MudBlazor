@namespace MudBlazor
@inherits ComponentBaseTextField


<MudFormControl HelperText="@HelperText">
    <MudInput InputType="@InputType" Class="mud-input-formcontrol" Variant="@Variant" @bind-Value="@Value" Placeholder="@Placeholder" Disabled=@Disabled ReadOnly="@ReadOnly" />
    <MudInputLabel Class="mud-input-label-formcontrol" Variant="@Variant">
        @Label
    </MudInputLabel>
</MudFormControl>

