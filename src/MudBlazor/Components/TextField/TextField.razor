@namespace MudBlazor
@inherits ComponentBaseTextField


<FormControl HelperText="@HelperText">
    <Input Type="@Type" Class="mud-input-formcontrol" Variant="@Variant" Value="@Value" Placeholder="@Placeholder" Disabled=@Disabled ReadOnly="@ReadOnly" />
    <InputLabel Class="mud-input-label-formcontrol" Variant="@Variant">@Label</InputLabel>
</FormControl>

