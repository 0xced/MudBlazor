@namespace MudBlazor
@inherits ComponentBaseTextField


<MudFormControl HelperText="@HelperText">
    <Input InputType="@InputType" Class="mud-input-formcontrol" Variant="@Variant" @bind-Value="@Value" Placeholder="@Placeholder" Disabled=@Disabled ReadOnly="@ReadOnly" />
    <InputLabel Class="mud-input-label-formcontrol" Variant="@Variant">
        @Label
    </InputLabel>
</MudFormControl>

