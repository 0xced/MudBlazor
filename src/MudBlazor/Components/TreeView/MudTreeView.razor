@namespace MudBlazor
@inherits MudComponentBase

<ul @attributes="UserAttributes" class="@Classname" style="@Style">
    <CascadingValue Value="@MudTreeRoot">
        @if (ItemTemplate != null)
        {
            @ItemTemplate
        }
        else
        {
            @ChildContent
        }
    </CascadingValue>
</ul>