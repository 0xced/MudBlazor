@namespace MudBlazor
@typeparam T
@inherits MudBaseSelectItem

<MudListItem @attributes="UserAttributes" Value="@ItemId" @onclick="@OnClicked" Icon="@CheckBoxIcon" Disabled="@Disabled" Class="@GetCssClasses()" Style="@Style">
    @if (ChildContent!=null)
    {
        @ChildContent
    }
    else
    {
        @DisplayString
    }
</MudListItem>


